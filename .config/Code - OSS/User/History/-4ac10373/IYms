#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias grep='grep --color=auto'
set PS1 '[\u@\h \W]\$ '

alias dot='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'

#if [ -z "$DISPLAY" ] && [ "$XDG_VTNR" = 1 ]; then
 # exec startx
#fi!!
